    <ui:Button text="生成发送消息" display-tooltip-when-elided="true" name="btnGenMsg" style="height: 20px; width: 114px; left: 30%; top: 10px; right: 50%; -unity-text-align: upper-center; white-space: nowrap;" />
    <ui:TextField picking-mode="Ignore" label="消息内容" name="txtMsgContent" style="height: 91px; flex-direction: column; top: 10px; justify-content: space-around; width: 346px;" />
    <ui:Button text="发送消息" display-tooltip-when-elided="true" name="btnSendMsg" enable-rich-text="true" style="height: 22px; width: 115px; bottom: 0; position: relative; left: 30%; top: 10px; right: 50%;" />
